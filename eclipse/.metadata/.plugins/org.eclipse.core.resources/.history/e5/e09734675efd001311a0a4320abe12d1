package com.tildenprep.derpmod.client;

import net.minecraft.creativetab.CreativeTabs;
import net.minecraft.entity.Entity;
import net.minecraft.item.Item;
import net.minecraft.item.ItemStack;
import net.minecraft.world.World;

public class UnfinishedRageIngot extends Item{
   
	public UnfinishedRageIngot(int damage){
        super();
        int damageVal = damage;
        setCreativeTab(CreativeTabs.tabMaterials);
        setMaxStackSize(64);
        setMaxDamage(6);
        this.setDamage(new ItemStack(this), damageVal);
        setUnlocalizedName("missingnoURI");
        setTextureName("derpMod:");
	}
	
	public void onUpdate(ItemStack par1ItemStack, World par2World, Entity par3Entity, int par4, boolean par5) {
		
	}
	
	
}
